{
  "name": "sdfz-demo-parser",
  "version": "1.4.0",
  "description": "Parser for SpringRTS .sdfz demo files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "tsc",
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "example": "ts-node example/test",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**.test.ts"
    ]
  },
  "author": "Jazcash",
  "license": "unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jazcash/sdfz-demo-parser.git"
  },
  "dependencies": {
    "jaz-signals": "^0.1.0",
    "node-gzip": "^1.1.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node-gzip": "^1.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1"
  }
}
